define(["jquery","common/utils/validationUtils"],function($,validator){var stringUtils={trim:function(str){return str.replace(/(^\s*)|(\s*$)/g,"")},getCommaNumber:function(nStr){var x,x1,x2;if(validator.isEmpty(nStr))return"";nStr+="";x=nStr.split(".");x1=x[0];x2=x.length>1?"."+x[1]:"";var rgx=/(\d+)(\d{3})/;while(rgx.test(x1))x1=x1.replace(rgx,"$1"+","+"$2");return x1+x2},toBoolean:function(str){if(validator.isEmpty(str))return false;if($.inArray(str.toLowerCase(),["true","1"])!==-1)return true;
return false},fromAlphabetToNumber:function(str){if(validator.isNotEmpty(str)&&str.charCodeAt(0)>64)return str.charCodeAt(0)-64;return""}};return stringUtils});